{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "plugins": [
    "node"
  ],
  "rules": {
    "typedef": false,
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-duplicate-variable": true,
    "no-empty": false,
    "eofline": false,
    "quotemark": [
      true, 
      "single"
    ],
    "prefer-const": "error"
  },
  "rulesDirectory": []
}
